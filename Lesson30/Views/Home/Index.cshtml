@*code block*@
@{
    ViewData["Title"] = "Main";
    var someText = $"Today is HTML lesson!:)," +
                   $" {DateTime.Now}";
    var todoList = new List<string>
    {
        "item1", "item2", "item3", "item4",
        "item5", "item6", "item7"
    };
}
@*html block*@
<div class="text-center">

    <ul class="list-group text-start">
        @foreach (var item in todoList)
        {
            <li class="list-group-item">
                @item
            </li>
        }
    </ul>


    <h1 class="display-4">@someText</h1>
    <h2>H2 text</h2>
    <br/>
    <h3>H3 text</h3>
    <p class="my-red-text">Some<br/>text</p>
    <p>some usual text</p>
    <a href="https://google.com" class="h1">
        Google it
    </a>
    <ul class="margin-big">
        <li>item1</li>
        <li>item2</li>
        <li>item3</li>
    </ul>
    <ol class="margin-big">
        <li>
            <h1>item1</h1>
        </li>
        <li>
            <b>item2</b>
        </li>
        <li>
            <i>item3</i>
        </li>
    </ol>
    @*<img src="https://www.google.com/images/branding/googlelogo/2x/googlelogo_light_color_272x92dp.png"*@

    <table class="table table-success table-striped-columns">
        <thead>
        <th>Header1</th>
        <th>Header2</th>
        </thead>
        <tbody>
        <tr>
            <td>1</td>
            <td>first item</td>
        </tr>
        <tr>
            <td>2</td>
            <td>second item</td>
        </tr>
        </tbody>
    </table>

    <table class="table table-success table-striped-columns">
        <tr>
            <td>
                <b>Header1</b>
            </td>
            <td>
                <b>Header2</b>
            </td>
        </tr>
        <tr>
            <td>1</td>
            <td>first item</td>
        </tr>
        <tr>
            <td>2</td>
            <td>second item</td>
        </tr>
    </table>
    <button type="button" class="btn btn-primary">Primary</button>
</div>